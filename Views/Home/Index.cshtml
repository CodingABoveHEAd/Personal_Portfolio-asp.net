@{
    ViewBag.Title = "Home";
}

<h1>Hello, I am [Your Name]</h1>
<p>Welcome to my portfolio!</p>

<h2>Skills</h2>
<ul>
    @foreach (var skill in ViewBag.Skills)
    {
        <li>@skill.Name - Level @skill.Level</li>
    }
</ul>

<h2>Education</h2>
<ul>
    @foreach (var edu in ViewBag.Educations)
    {
        <li>@edu.Degree, @edu.Institute (@edu.Year)</li>
    }
</ul>
