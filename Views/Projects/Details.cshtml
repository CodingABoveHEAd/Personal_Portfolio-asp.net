@model Personal_Portfolio2.Models.Project
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Project Details";
}
<h2>Project Details</h2>
<dl class="dl-horizontal">
    <dt>Id</dt>
    <dd>@Model.Id</dd>
    <dt>Title</dt>
    <dd>@Model.Title</dd>
    <dt>Description</dt>
    <dd>@Model.Description</dd>
    
</dl>
<p>@Html.ActionLink("Back", "Projects", "Admin", null, new { @class = "btn btn-link" })</p>
